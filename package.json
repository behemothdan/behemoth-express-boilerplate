{
  "name": "behemoth-express-boilerplate",
  "version": "1.0.0",
  "description": "My boilerplate for an Express API using Typescript for ease of setting up future projects.",
  "main": "server.js",
  "dependencies": {
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "moment": "^2.24.0",
    "mongoose": "^5.9.5"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.3",
    "@types/mongoose": "^5.7.6",
    "@types/node": "^13.9.1",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "dotenv": "^8.2.0",
    "nodemon": "^2.0.2",
    "ts-mongoose": "0.0.21",
    "ts-node": "^8.6.2",
    "tslint": "^6.1.0",
    "typescript": "^3.8.3"
